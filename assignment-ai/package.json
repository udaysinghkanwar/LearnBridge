{
  "name": "assignment-ai",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "npm -w @assignment-ai/api run dev",
    "dev:web": "npm -w @assignment-ai/web run dev",
    "build": "npm -w @assignment-ai/shared run build && npm -w @assignment-ai/api run build && npm -w @assignment-ai/web run build"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5"
  }
}

